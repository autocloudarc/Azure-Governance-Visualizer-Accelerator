{
  "displayName": "Restrict resource type usage to conform to trade regulations.",
  "policyType": "BuiltIn",
  "mode": "All",
  "description": "Restrict which resource types can be deployed in your environment in compliance with U.S. and EU trade laws and regulations.",
  "metadata": {
    "version": "1.0.0",
    "category": "System Policy"
  },
  "policyRule": {
    "if": {
      "anyOf": [
        {
          "field": "type",
          "like": "Microsoft.Kusto/*"
        },
        {
          "field": "type",
          "like": "Microsoft.Dashboard/*"
        },
        {
          "field": "type",
          "like": "Microsoft.ProgrammableConnectivity/*"
        },
        {
          "field": "type",
          "like": "Microsoft.Quantum/*"
        },
        {
          "field": "type",
          "like": "Microsoft.ServiceFabric/*"
        },
        {
          "field": "type",
          "like": "Microsoft.Synapse/*"
        },
        {
          "field": "type",
          "like": "Microsoft.PowerBIDedicated/*"
        }
      ]
    },
    "then": {
      "effect": "Deny"
    }
  }
}
